<div>
  <h3 class="font-extrabold text-2xl text-black dark:text-white">Stories</h3>
  <div id="stories-carousel" class="relative w-full" data-carousel="slide" tabindex="-1">
    <!-- Stories carousel wrapper -->
    <ul class="relative h-12 overflow-hidden rounded-lg md:h-20 w-[calc(100%+14px)]">
      <li class="md:pr-3">
        <div class="md:w-20 md:h-20 w-12 h-12 rounded-full relative border-2 border-dashed grid place-items-center bg-slate-200 border-slate-300 dark:border-slate-700 dark:bg-dark2">
          <svg class="w-6 h-6 text-gray-800 dark:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
            <path fill-rule="evenodd" d="M7.5 4.586A2 2 0 0 1 8.914 4h6.172a2 2 0 0 1 1.414.586L17.914 6H19a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h1.086L7.5 4.586ZM10 12a2 2 0 1 1 4 0 2 2 0 0 1-4 0Zm2-4a4 4 0 1 0 0 8 4 4 0 0 0 0-8Z" clip-rule="evenodd"/>
          </svg>
        </div>
      </li>
      <% story = Hash.new(title: 'Story 1', image: 'https://via.placeholder.com/150', url: '#') %>
      <%= render partial: 'dashboard/story', locals: { story: story } %>
    </ul>
  </div>
</div>
